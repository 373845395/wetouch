<template>
  <ui-view id="ui-app">
    <ui-slide-menu width="280" threshold="30" touch="{{ pageConfig.touchSlideMenu }}" ui:model="showSlideMenu">

      <ui-view slot="menu" class="menu">

        <ui-view class="slide_top" style="{{{ 'padding-top': 20+barHeight+'px' }}}">
          <ui-image
          src="{{ require('#/images/logo-white.png') }}"
          width="100%"
          ></ui-image>
          <!-- <ui-view>TouchUI Pro</ui-view>
          <ui-text>标准版</ui-text> -->
        </ui-view>

        <ui-view class="slide_top slide_content" bindtap="closeMenu">
          <ui-image
          src="{{ require('#/images/sideBar/shounaxiang.png') }}"
          width='50'
          height='50'
          ></ui-image>
          <ui-view>收集想法 · 接纳任务</ui-view>
        </ui-view>
        <ui-row>
          <ui-col vertical-align="middle" aling="center" space-right="5" bindtap="closeMenu">
            <ui-view class="slide_top slide_content">
              <ui-image
              src="{{ require('#/images/sideBar/gongsi.png') }}"
              width='20'
              height='20'
              ></ui-image>
              <ui-view>创建公司</ui-view>
            </ui-view>
          </ui-col>
          <ui-col vertical-align="middle" aling="center" space-left="5" bindtap="closeMenu">
            <ui-view class="slide_top slide_content">
              <ui-image
              src="{{ require('#/images/sideBar/tongshi.png') }}"
              width='20'
              height='20'
              ></ui-image>
              <ui-view>邀请同事</ui-view>
            </ui-view>
          </ui-col>
        </ui-row>

        <ui-row>
          <ui-col vertical-align="middle" aling="center" space-right="5" bindtap="closeMenu">
            <ui-view class="slide_top slide_content">
              <ui-image
              src="{{ require('#/images/sideBar/guanyu.png') }}"
              width='20'
              height='20'
              ></ui-image>
              <ui-view>关于我们</ui-view>
            </ui-view>
          </ui-col>
          <ui-col vertical-align="middle" aling="center" space-left="5" bindtap="closeMenu">
            <ui-view class="slide_top slide_content">
              <ui-image
              src="{{ require('#/images/sideBar/zixun.png') }}"
              width='20'
              height='20'
              ></ui-image>
              <ui-view>在线咨询</ui-view>
            </ui-view>
          </ui-col>
        </ui-row>

        <ui-view class="slide_bottom">
          <ui-row height="55">
            <ui-col vertical-align="middle" aling="center" bindtap="closeMenu">
              <ui-view>
                <ui-image src="{{ require('#/images/sideBar/sousuo.png') }}"></ui-image>
              </ui-view>
            </ui-col>
            <ui-col vertical-align="middle" aling="center" bindtap="closeMenu">
              <ui-view>
                <ui-image src="{{ require('#/images/sideBar/sz.png') }}"></ui-image>
              </ui-view>
            </ui-col>
            <ui-col vertical-align="middle" aling="center" bindtap="closeMenu('close')">
              <ui-view>
                <ui-image src="{{ require('#/images/sideBar/guanji.png') }}"></ui-image>
              </ui-view>
            </ui-col>
          </ui-row>
        </ui-view>

      </ui-view>

      <ui-view slot="panel">
      <!-- 通用导航栏 -->
      <ui-nav-bar
        ui:if="pageConfig"
        ui:show="navigationBarVisible"
        style="{{ navStyle }}">
        <ui-row>
          <ui-col width="80">
            <span class="back" bindtap="handleBack" ui:show="{{showBack}}">
              <ui-icon type="arrow-left" font-size="24"></ui-icon>返回
            </span>
            <span class="back" ui:show="{{!showBack}}" bindtap="handleNavIconTap">
              <ui-icon type="list" color="#2B333B" size="16"></ui-icon>
            </span>
          </ui-col>
          <ui-col width="{{WIN_WIDTH-160}}" align="center" content-direction="row" class="nav-bar-center">
            <ui-view class="nav-bar-center-loading" style="display: none;"></ui-view>
            <span class="nav-bar-center-text">{{ $t(winConfig.navigationBarTitleText) }}</span>
          </ui-col>
          <ui-col width="80">

          </ui-col>
        </ui-row>
      </ui-nav-bar>

      <!-- 页面路由 -->
      <router-view class="router-view">
      </router-view>

      <!-- 底部标签栏 -->
      <ui-tab-bar
        bindbeforechange="handleTabBeforeChange"
        bindchange="handleTabChange"
        ui:if="isTabBar"
        ui:model="tabIndex"
        style="{{ tabBarStyle }}"
        id="{{ tabBar.id }}"
        group-id="{{ tabBar.groupId }}"
        icon-size="{{ tabBar.iconSize }}"
        font-size="{{ tabBar.fontSize }}"
        position="{{ tabBar.position }}"
        list="{{ tabBarList }}"
        paths="{{ tabBar.paths }}"
        background-color="{{ tabBar.backgroundColor }}"
        border-color="{{ tabBar.borderColor }}"
        color="{{ tabBar.color }}"
        selected-color="{{ tabBar.selectedColor }}">
    </ui-tab-bar>

    <!-- tabbar个性按钮示例用到：弹出图标导航菜单 -->
    <ui-fixed-view top="0" left="0" right="0" style="{{{ 'z-index': '1000' ,'display':tabbaricon}}}">
      <ui-pop-icon mask-hide-delay="500" blur="light" hide-on-tap="{{hideOnTap}}"  bindhide="hidepopicon($event)" bindmaskhide="popiconMaskhide"  left="{{offsetX}}" ui:model="toggle" class="pop-icon-demo" animate="bounce" >
        <ui-pop-icon-item item-color="#f9dc66" top="-85" left="{{0 - offsetX / 2 - 48}}">
          <ui-view class="icon_view">
            <ui-image src="{{require('./images/icon1.png')}}" width="30"></ui-image>
          </ui-view>
        </ui-pop-icon-item>
        <ui-pop-icon-item item-color="#f9dc66" top="-85" left="{{0 - 24}}">
          <ui-view class="icon_view">
            <ui-image src="{{require('./images/icon2.png')}}" width="30"></ui-image>
          </ui-view>
        </ui-pop-icon-item>
        <ui-pop-icon-item item-color="#f9dc66" top="-85" left="{{0 + offsetX / 2}}">
          <ui-view class="icon_view">
            <ui-image src="{{require('./images/icon3.png')}}" width="30"></ui-image>
          </ui-view>
        </ui-pop-icon-item>
      </ui-pop-icon>
    </ui-fixed-view>
    <ui-fixed-view bottom="25" left="0" right="0" style="{{fixedzindex}}">
      <ui-view class="{{togglestyle}}" style="text-align:center;width:35px;height:35px;margin:0 auto" bindtap="hidepopicon($event)">
        <ui-icon type="plus" color="#555" size="24"></ui-icon>
      </ui-view>
    </ui-fixed-view>


      </ui-view>
    </ui-slide-menu>

    <!-- 搜索 -->
      <ui-mask class="full-screen-box" top="0" ui:model="globalData.searchShow"  bindhide="searchHideHandle" bindshow="searchShowHandle">
        <ui-view class="bar" style="{{{ height:barHeight+'px' }}}"></ui-view>
        <ui-row height="46" class="search_bg">
          <ui-col vertical-align="middle" align="center" space-left="15">
            <ui-view class="search">
              <ui-row height="30">
                <ui-col vertical-align="middle" align="center" width="30">
                  <ui-icon type="magnifier" size="16" color="#B5B5B5"></ui-icon>
                </ui-col>
                <ui-col vertical-align="middle">
                  <ui-form bindsubmit="searchSubmit" action="/search">
                    <ui-input type="search" name="search" ui:model="{{ searchValue }}"  placeholder="搜索" focus="{{ globalData.searchShow }}" show-clear="false"></ui-input>
                  </ui-form>
                </ui-col>
              </ui-row>
            </ui-view>
          </ui-col>
          <ui-col vertical-align="middle" align="center" width="50" bindtap="hideFullScreenSearch">
            <ui-view class="cancel">取消</ui-view>
          </ui-col>
        </ui-row>
        <ui-view class="search_content" style="{{{ height:height+'px' }}}">
          <!-- 历史搜索 -->
          <ui-view class="search_history" ui:if="{{ historyContent.length<1 ? false : true }}">
            <ui-row height="20" space-bottom="20">
              <ui-col vertical-align="middle">历史搜索</ui-col>
              <ui-col vertical-align="middle" align="right" width="30" bindtap="deleteHistory">
                <ui-icon type="delete2" size="16" color="#C9C9C9"></ui-icon>
              </ui-col>
            </ui-row>
            <ui-view class="history_content">
              <ui-view ui:for="{{ historyContent }}" bindtap="historyTap(index)">{{ item }}</ui-view>
            </ui-view>
          </ui-view>
          <ui-view class="search_history">
            <ui-row height="20" space-bottom="20">
              <ui-col vertical-align="middle">大家都在搜</ui-col>
            </ui-row>
            <ui-view class="history_content">
              <ui-tags
              data="{{ hotList }}"
              bindchange="hotTap"
              selected-class="select-class"
              ></ui-tags>
            </ui-view>
          </ui-view>
        </ui-view>
      </ui-mask>

    <!-- 分享 -->
    <ui-mask ui:model="{{globalData.shareMask}}" class="share_mask" hide-delay="400" top="0" blur="light" bindshow="maskShow" bindtap="columnHide">
      <ui-view class="column_wrap {{ className }}">

        <ui-view class="column_content" ui:for="{{ shareColumn }}" style="{{{ 'animation-duration':animationDuration[index] }}}" bindtap="maskHide(item.share)">
          <ui-view >
            <ui-view style="{{{ 'background-color':item.bgColor }}}">
              <ui-icon type="{{ item.icon }}" size="30" color="#fff"></ui-icon>
            </ui-view>
            <ui-text block>{{ item.name }}</ui-text>
          </ui-view>
        </ui-view>

        <ui-view class="close" bindtap="maskHide('plus')">
          <ui-icon type="close" size="20" color="#5E5E5E"></ui-icon>
        </ui-view>

      </ui-view>
    </ui-mask>


    <!-- 音频播放 -->
    <ui-fixed-view class="audio_bottom {{ fixedClassName }}" ui:if="{{ globalData.audioShow }}">
      <ui-row height="46">
        <ui-col vertical-align="middle" align="center" width="{{ leftPanelWidth }}" class="{{ leftPanelClass }}" bindtap="audioPanelHide">
          <ui-icon type="error" size="18" color="#fff"></ui-icon>
        </ui-col>
        <ui-col vertical-align="middle" align="center" width="30">
          <ui-image
            src="{{ require('#/images/indexList/t4.png') }}"
            width="30"
            height="30"
          ></ui-image>
        </ui-col>
        <ui-col vertical-align="middle" align="left" space-left="5" bindtap="audioPopupShow">
          <ui-view>{{ songName }}</ui-view>
          <ui-text>{{ maxFormat(sliderMax) }}</ui-text>
        </ui-col>
        <ui-col vertical-align="middle" align="center" width="30" bindtap="audioPopupShow">
          <ui-icon type="arrow-up" size="18" color="#fff"></ui-icon>
        </ui-col>
        <ui-col vertical-align="middle" align="center" width="50" bindtap="playOff">
          <ui-icon type="{{ playAudioIcon }}" size="16" color="#fff" class="paly_icon"></ui-icon>
          <ui-circular
          radius="15"
          top="15"
          left="15"
          color="#FF9900"
          line-cap="round"
          value="{{ 100 / sliderMax * sliderValue }}"
          line-width="2"
          ></ui-circular>
        </ui-col>
      </ui-row>
    </ui-fixed-view>

    <!--音频播放模版 -->
    <ui-popup height="100%" ui:model="{{ audioPopup }}">
      <ui-view class="audio_content" style="{{{ height: contentHeight+barHeight+46+'px' }}}">
        <ui-view class="audio_head" style="{{{ 'padding-top':barHeight+'px' }}}" bindtap="audioPopupHide">
          <ui-icon type="arrow-down" color="#666666" size="24"></ui-icon>
        </ui-view>
        <ui-view class="songImg" ui:if="{{audioPopup}}">
          <ui-image src="{{ imgSrc }}" height="100%" mode="aspectFit"></ui-image>
        </ui-view>
        <ui-view class="songText">
          <ui-view class="song_title">
            <ui-text block class="text text1">{{songName}}</ui-text>
            <ui-text block class="text">{{singerName}}</ui-text>
          </ui-view>
        </ui-view>
        <ui-view class="songText2">
          <ui-view class="control">
            <ui-slider style="margin:0 auto" value-pos="left" show-max-value="true" show-value="true" value="{{sliderValue}}" 
              max-format="{{maxFormat}}"
              value-format="{{valueFormat}}"
              tooltip-format="{{tooltipFormat}}"
              width="{{winWidth-50}}"
              thumb-style="{{thumbStyle}}"
              thumb-down-style="{{thumbDownStyle}}"
              background-color="#E4E4E4" 
              active-color="#FF9D1F" 
              slider-weight="2"
              min="{{sliderMin}}" 
              max="{{sliderMax}}"
              bindchange="sliderChange"
              >
            </ui-slider>
          </ui-view>
        </ui-view>
        <ui-view class="songPlay">
          <ui-row class="play_win">
              <ui-col span='2' align="center" bindtap="rewind">
                  <ui-icon type="rewind" size="24" color="#666666" ></ui-icon>
              </ui-col>
              <ui-col span='2' align="center" bindtap="onPrev">
                  <ui-icon type="presong" size="24" color="#666666"></ui-icon>
              </ui-col>
              <ui-col span='4' align="center" bindtap="playMusic">
                  <!-- <ui-icon name="{{play}}" size="60" symbol></ui-icon> -->
                  <ui-image src="{{ svgSrc }}" width="50"></ui-image>
              </ui-col>
              <ui-col span='2' align="center" bindtap="onNext">
                  <ui-icon type="nextsong" size="24" color="#666666"></ui-icon>
              </ui-col>
              <ui-col span='2' align="center" bindtap="fastForward">
                  <ui-icon type="fastforward" size="24" color="#666666" ></ui-icon>
              </ui-col>
          </ui-row>
        </ui-view>
        <ui-view class="songBar">
          <ui-view class="songBar_content">
              <ui-row class="bottom_bar">
                  <ui-col align="center" vertical-align="middle" bindtap="listShow">
                      <ui-view>
                          <ui-icon type="playlist" size="24" color="#666666"></ui-icon>
                          <ui-text block>曲单</ui-text>
                      </ui-view>
                  </ui-col>
                  <ui-col align="center" vertical-align="middle" bindtap="showToast('文稿功能未实现')" >
                      <ui-view >
                          <ui-icon type="draft" size="24" color="#666666"></ui-icon>
                          <ui-text block>文稿</ui-text>
                      </ui-view>
                  </ui-col>
                  <ui-col align="center" vertical-align="middle" bindtap="likeMusic">
                      <ui-view class="animated {{likeClass}}">
                          <ui-icon type="{{praise}}" size="24" color="{{likeColor}}"></ui-icon>
                          <ui-text block>{{likeNum}}</ui-text>
                      </ui-view>
                  </ui-col>
                  <ui-col align="center" vertical-align="middle" bindtap="showToast('下载功能未实现')">
                      <ui-view>
                          <ui-icon type="playdown" size="24" color="#666666"></ui-icon>
                          <ui-text block>下载</ui-text>
                      </ui-view>
                  </ui-col>
                  <ui-col align="center" vertical-align="middle" bindtap="showToast('更多功能未实现')">
                      <ui-view>
                          <ui-icon type="more" size="24" color="#666666"></ui-icon>
                          <ui-text block>更多</ui-text>
                      </ui-view>
                  </ui-col> 
              </ui-row>
          </ui-view>
        </ui-view>
        <!-- 音乐列表 -->
        <ui-popup ui:model="{{ show }}" position="bottom" width="100%" height="300" class="audio_play_list">
          <ui-view class="audio_play_list_wrap">
              <ui-row height="50" border-bottom="{{true}}" class="list_title">
                  <ui-col align="left" vertical-align="middle" space-left="10">
                      <ui-text> 音频列表 </ui-text>
                  </ui-col>
                  <ui-col align="right" vertical-align="middle" space-right="10" bindtap="listShow">
                      <ui-icon  type="x" ></ui-icon>
                  </ui-col>
              </ui-row>
              <ui-view class="list_content">
                  <ui-view>
                      <ui-view ui:for="{{musicList}}">
                          <ui-view class="song_name" bindtap="listPlay(index)" :class="{nowSong:index===curIndex}">
                              <ui-text class="songname">{{musicList[index].epname}}</ui-text>
                              <ui-text class="singername">{{musicList[index].singer}}</ui-text>
                          </ui-view>
                      </ui-view>
                  </ui-view>
              </ui-view>
          </ui-view>
        </ui-popup>
      </ui-view>
    </ui-popup>

  </ui-view>

</template>
