<!--stackedBar.ui-->
<template>
  <ui-page>
    <ui-view class="sample_title">堆积柱状图：基本使用</ui-view>
    <ui-stacked-bar
    value="{{stackedBarData.data}}"
    width="{{winWidth}}"
    height="250"
    is-animated="{{true}}"
    colorSchema="{{stackedBarData.colorSchema}}"
    ></ui-stacked-bar>
    <ui-legend
    value="{{legendData.legend}}"
    width="{{winWidth}}"
    height="50"
    is-horizontal="{{true}}"
    is-animated="{{true}}"
    colorSchema="{{legendData.colorSchema}}"
  ></ui-legend>

    <ui-view class="sample_title">堆积柱状图：点击事件</ui-view>
    <ui-stacked-bar
    value="{{stackedBarData.data}}"
    width="{{winWidth}}"
    height="250"
    is-animated="{{true}}"
    is-darker="{{true}}"
    bindcustommove="custommove"
    colorSchema="{{stackedBarData.colorSchema}}"
    ></ui-stacked-bar>
    <ui-legend
    value="{{legendData.legend}}"
    width="{{winWidth}}"
    height="50"
    is-horizontal="{{true}}"
    is-animated="{{true}}"
    textLetterSpacing ="0.5"
    colorSchema="{{legendData.colorSchema}}"
  ></ui-legend>
    <ui-text block>{{clickText}}</ui-text>

    <ui-view class="sample_title">堆积柱状图：使用tootip</ui-view>
    <ui-stacked-bar
    value="{{stackedBarData.data}}"
    width="{{winWidth}}"
    height="250"
    is-animated="{{true}}"
    isDarker="{{true}}"
    tooltip="{{true}}"
    bodyFillColor="rgba(0,0,0,.7)"
    tooltipWidth="150"
    titleFillColor="#fff"
    textFillColor="#fff"
    tooltipTextColor="#fff"
    borderStrokeColor="none"
    colorSchema="{{stackedBarData.colorSchema}}"
    ></ui-stacked-bar>
    <ui-legend
    value="{{legendData.legend}}"
    width="{{winWidth}}"
    height="50"
    is-horizontal="{{true}}"
    is-animated="{{true}}"
    textLetterSpacing ="0.5"
    colorSchema="{{legendDataEn.colorSchema}}"
  ></ui-legend>


    <ui-view class="sample_title">堆积柱状图：百分比</ui-view>
    <ui-stacked-bar
    value="{{stackedBarData2.data}}"
    width="{{winWidth}}"
    height="250"
    is-animated="{{true}}"
    nameLabel="date"
    stackLabel="stack"
    valueLabel="views"
    colorSchema="{{stackedBarData2.colorSchema}}"
    percentageAxisToMaxRatio=1.6
    hasPercentage="{{true}}"
    enablePercentageLabels="{{true}}"
    valueLabelFormat="%"
    ></ui-stacked-bar>
    <ui-legend
    value="{{legendData.legend}}"
    width="{{winWidth}}"
    height="50"
    is-horizontal="{{true}}"
    is-animated="{{true}}"
    textLetterSpacing ="0.5"
    colorSchema="{{legendData.colorSchema}}"
  ></ui-legend>

    
  <ui-view class="sample_title">堆积条形图：基本使用</ui-view>
    <ui-stacked-bar
    value="{{stackedBarData.data}}"
    width="{{winWidth}}"
    height="150"
    is-animated="{{true}}"
    is-horizontal = "{{true}}"
    colorSchema="{{stackedBarData.colorSchema}}"
    margin="{{margin}}"
    ></ui-stacked-bar>
    <ui-legend
    value="{{legendData.legend}}"
    width="{{winWidth}}"
    height="50"
    is-horizontal="{{true}}"
    is-animated="{{true}}"
    textLetterSpacing ="0.5"
    colorSchema="{{legendData.colorSchema}}"
  ></ui-legend>


  <ui-view class="sample_title">堆积条形图：点击事件</ui-view>
  <ui-stacked-bar
  value="{{stackedBarData.data}}"
  width="{{winWidth}}"
  height="150"
  is-animated="{{true}}"
  is-darker="{{true}}"
  is-horizontal = "{{true}}"
  bindcustomover="custommove"
  colorSchema="{{stackedBarData.colorSchema}}"
  margin="{{margin}}"
  ></ui-stacked-bar>
  <ui-legend
  value="{{legendData.legend}}"
  width="{{winWidth}}"
  height="50"
  is-horizontal="{{true}}"
  is-animated="{{true}}"
  textLetterSpacing ="0.5"
  colorSchema="{{legendData.colorSchema}}"
></ui-legend>
  <ui-text block>{{clickText}}</ui-text>

  <ui-view class="sample_title">堆积条形图：使用tootip</ui-view>
  <ui-stacked-bar
  value="{{stackedBarData.data}}"
  width="{{winWidth}}"
  height="150"
  is-animated="{{true}}"
  isDarker="{{true}}"
  tooltip="{{true}}"
  is-horizontal = "{{true}}"
  bodyFillColor="rgba(0,0,0,.7)"
  tooltipWidth="150"
  titleFillColor="#fff"
  textFillColor="#fff"
  tooltipTextColor="#fff"
  borderStrokeColor="none"
  colorSchema="{{stackedBarData.colorSchema}}"
  margin="{{margin}}"
  ></ui-stacked-bar>
  <ui-legend
  value="{{legendData.legend}}"
  width="{{winWidth}}"
  height="50"
  is-horizontal="{{true}}"
  is-animated="{{true}}"
  textLetterSpacing ="0.5"
  colorSchema="{{legendData.colorSchema}}"
></ui-legend>


  <ui-view class="sample_title">堆积条形图：百分比</ui-view>
  <ui-stacked-bar
  value="{{stackedBarData2.data}}"
  width="{{winWidth}}"
  height="150"
  nameLabel="date"
  stackLabel="stack"
  valueLabel="views"
  is-horizontal = "{{true}}"
  is-animated="{{true}}"
  colorSchema="{{stackedBarData.colorSchema}}"
  percentageAxisToMaxRatio=1.6
  hasPercentage="{{true}}"
  enablePercentageLabels="{{true}}"
  margin="{{margin}}"
  valueLabelFormat="%"
  ></ui-stacked-bar>
  <ui-legend
  value="{{legendData.legend}}"
  width="{{winWidth}}"
  height="50"
  is-horizontal="{{true}}"
  is-animated="{{true}}"
  textLetterSpacing ="0.5"
  colorSchema="{{legendData.colorSchema}}"
></ui-legend>

  </ui-page>
</template>
